{
  "name": "@myetherwallet/mew-components",
  "version": "1.3.7",
  "description": "MEW Components",
  "main": "dist/mew-components.umd.js",
  "module": "dist/mew-components.esm.js",
  "unpkg": "dist/mew-components.min.js",
  "browser": {
    "./sfc": "src/mew-components.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint && eslint . --ext .js --ext .vue --fix",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build-bundle": "vue-cli-service build --target lib --name mew-components ./src/wrapper.js",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/mew-components.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/mew-components.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/mew-components.min.js",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./src",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpag es",
    "eslint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/babel-preset-app": "^4.4.6",
    "@vue/compat": "^3.1.0",
    "all": "0.0.0",
    "bignumber.js": "^9.0.0",
    "chart.js": "^2.9.3",
    "core-js": "3.6.5",
    "eslint-config-eslint": "^6.0.0",
    "fibers": "^5.0.0",
    "git-url-parse": "^11.1.2",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^5.0.1",
    "minimist": "^1.2.5",
    "pinia": "^2.0.23",
    "rollup-plugin-uglify-es": "0.0.1",
    "style-loader": "^1.2.1",
    "vue": "^3.1.0",
    "vue-chartjs": "^3.5.0",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-loader": "^16.0.0",
    "vue-router": "^4.1.5",
    "vue-style-loader": "^4.1.2",
    "vue-tippy": "^4.13.0",
    "vuetify": "^3.0.0-beta.13",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@mdi/font": "^5.1.45",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-url": "6.0.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-backgrounds": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/source-loader": "^6.5.12",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/vue3": "^6.5.12",
    "@vue/cli": "^5.0.8",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/web-component-wrapper": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "prettier": "^1.18.2",
    "rollup": "^2.3.0",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^7.0.2",
    "style-resources-loader": "^1.3.3",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "webpack-plugin-vuetify": "^2.0.0-alpha.13"
  },
  "files": [
    "dist",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ]
}
